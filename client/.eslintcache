n            <Media.Body>\r\n                <h5>{book.name}</h5>\r\n                <h5>{book.author}</h5>\r\n            </Media.Body>\r\n        </Media>\r\n        </>\r\n    )\r\n}","C:\\Users\\ASUS\\WebstormProjects\\visual-summary\\client\\src\\components\\media\\HardDriveMedia.js",[],"C:\\Users\\ASUS\\WebstormProjects\\visual-summary\\client\\src\\components\\media\\MusicAlbumMedia.js",[],"C:\\Users\\ASUS\\WebstormProjects\\visual-summary\\client\\src\\model\\requests\\MediaModelRestAPI.js",["246","247"],"C:\\Users\\ASUS\\WebstormProjects\\visual-summary\\client\\src\\model\\requests\\UserActionRestAPI.js",["248","249","250"],"import React ,{useEffect, useState} from 'react'\r\nimport axios from 'axios'\r\nimport { getToken } from '../hooks/authentication_provider'\r\n\r\nexport function createUseMedia(media_id){\r\n    const url = 'http://127.0.0.1:5000/private/user/useMedia'\r\n    const body = {media_id: media_id}\r\n    const headers = {headers : {\"x-auth-token\": getToken()}}\r\n    return axios.put(url, body, headers)\r\n}\r\n\r\nexport function releaseUseMedia(media_id){\r\n    const url = 'http://127.0.0.1:5000/private/user/releaseMedia'\r\n    const body = {media_id: media_id}\r\n    const headers = {headers : {\"x-auth-token\": getToken()}}\r\n    return axios.put(url, body, headers)\r\n}","C:\\Users\\ASUS\\WebstormProjects\\visual-summary\\client\\src\\model\\dummies\\MediaDummiesModel.js",[],"C:\\Users\\ASUS\\WebstormProjects\\visual-summary\\client\\src\\components\\media\\PictureAlbumMedia.js",[],"C:\\Users\\ASUS\\WebstormProjects\\visual-summary\\client\\src\\components\\editors\\BookEditor.js",[],"C:\\Users\\ASUS\\WebstormProjects\\visual-summary\\client\\src\\components\\editors\\MusicAlbumEditor.js",[],"C:\\Users\\ASUS\\WebstormProjects\\visual-summary\\client\\src\\components\\editors\\HardDriveEditor.js",[],"C:\\Users\\ASUS\\WebstormProjects\\visual-summary\\client\\src\\components\\editors\\PictureAlbumEditor.js",[],"C:\\Users\\ASUS\\WebstormProjects\\visual-summary\\client\\src\\repositories\\TextRepository.js",["251","252","253","254","255","256","257","258"],"C:\\Users\\ASUS\\WebstormProjects\\visual-summary\\client\\src\\model\\requests\\TextModelRestAPI.js",["259","260"],"import React ,{useEffect, useState} from 'react'\r\nimport axios from 'axios'\r\nimport { getToken } from '../hooks/authentication_provider'\r\n\r\nfunction useAxiosGet(url, headers){    \r\n    const [request, setRequest] = useState({\r\n        loading: false,\r\n        data: [],\r\n        error: false\r\n    })\r\n\r\n    useEffect(() => {\r\n        setRequest({\r\n            loading: true,\r\n            data: [],\r\n            error: false\r\n        })\r\n        axios.get(url, headers).then( response => {\r\n            setRequest({\r\n                loading: false,\r\n                data: response.data,\r\n                error: false\r\n            })\r\n        }).catch( () => {\r\n            setRequest({\r\n                loading: false,\r\n                data: [],\r\n                error: true\r\n            })\r\n        })\r\n    },[url])\r\n\r\n    return request\r\n}\r\n\r\nexport function useGetAllText(){\r\n    const url = 'http://127.0.0.1:5000/texts'\r\n    const headers = {headers: {\"x-auth-token\": getToken()}}\r\n    return useAxiosGet(url, headers)\r\n}\r\n\r\nexport function useGetTextById(id){\r\n    const url = `http://127.0.0.1:5000/texts/${id}`\r\n    const headers = {headers: {\"x-auth-token\": getToken()}}\r\n    return useAxiosGet(url, headers, false)\r\n}\r\n\r\nexport function useGetTextWeights(id){\r\n    const url = `http://127.0.0.1:5000/texts/${id}/weights`\r\n    const headers = {headers: {\"x-auth-token\": getToken()}}\r\n    return useAxiosGet(url, headers, false)\r\n}\r\n\r\nexport function createText(props){\r\n    const url = 'http://127.0.0.1:5000/texts'\r\n    const body = props\r\n    const headers = {headers : {\"x-auth-token\": getToken()}}\r\n    return axios.post